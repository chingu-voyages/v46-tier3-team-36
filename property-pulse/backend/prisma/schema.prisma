generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                            Int            @id @default(autoincrement())
  name                          String?
  password                      String?
  email                         String         @unique
  emailVerified                 DateTime?
  image                         String?
}
